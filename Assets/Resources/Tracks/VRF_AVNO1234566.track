	  <disc name="reward_dot" height="-4.95" material="plain_white.png">
		<facing x="0" y="-1" z="0"/>
		<scale x="25" y ="1" z="25"/>
	  </disc>	  

      <occupationzone name="reward_zone" active="true" mintime="0">
